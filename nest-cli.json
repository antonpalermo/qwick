{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/qwick/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/qwick/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/qwick",
  "projects": {
    "qwick": {
      "type": "application",
      "root": "apps/qwick",
      "entryFile": "main",
      "sourceRoot": "apps/qwick/src",
      "compilerOptions": {
        "tsConfigPath": "apps/qwick/tsconfig.app.json"
      }
    },
    "qwick-backend": {
      "type": "application",
      "root": "apps/qwick-backend",
      "entryFile": "main",
      "sourceRoot": "apps/qwick-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/qwick-backend/tsconfig.app.json"
      }
    }
  }
}
