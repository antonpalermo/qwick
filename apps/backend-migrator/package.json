{
  "name": "backend-migrator",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "with-env": "dotenv -e ../../.env",
    "build": "tsup index.ts --dts --minify --format cjs --out-dir dist",
    "db:push": "pnpm with-env -- prisma db push",
    "db:migrate": "pnpm with-env -- prisma migrate dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "kysely": "^0.27.5"
  },
  "devDependencies": {
    "prisma": "^6.1.0",
    "prisma-kysely": "^1.8.0",
    "tsup": "^8.3.5",
    "typescript": "~5.6.2"
  },
  "license": "MIT"
}
